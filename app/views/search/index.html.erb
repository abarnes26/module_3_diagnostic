<h2>Search Results</h2><br>
<div class="station-list">
  <% @stations.each do |station| %>
  <div class="station">
    Name: <%= station.name %><br>
    Address: <%= station.address %><br>
    Fuel Types: <%= station.fuel_types %><br>
    Distance: <%= station.distance %><br>
    Access Times: <%= station.access_times %><br><br>
  </div>
  <% end %>
</div>
